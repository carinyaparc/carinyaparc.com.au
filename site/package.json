{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": "^22.17.0 || ^23.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 0.1%"
  ],
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "optimize-images": "node scripts/optimize-images.mjs"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "latest",
    "@hookform/resolvers": "^5.2.2",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@next/mdx": "^15.5.4",
    "@next/third-parties": "^15.5.4",
    "@repo/ui": "workspace:*",
    "@sentry/nextjs": "^10.15.0",
    "@tailwindcss/typography": "^0.5.19",
    "@types/mdx": "^2.0.13",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.5.0",
    "clsx": "^2.1.1",
    "date-fns": "4.1.0",
    "framer-motion": "latest",
    "fs": "0.0.1-security",
    "gray-matter": "^4.0.3",
