{
  "name": "carinya-parc-monorepo",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": "^22.17.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "site:dev": "turbo run dev --filter=site",
    "site:build": "turbo run build --filter=site",
    "lint": "turbo run lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "turbo run test:ci",
    "test:smoke": "turbo run test:smoke",
    "test:integration": "turbo run test:integration",
    "test:unit": "turbo run test:unit",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write --log-level=warn .",
    "format:check": "prettier --check --log-level=warn .",
    "clean": "turbo run clean"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitest/coverage-istanbul": "^4.0.3",
    "@vitest/coverage-v8": "^4.0.3",
    "eslint": "^9.38.0",
    "jsdom": "^27.0.1",
    "msw": "^2.11.6",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3",
    "vitest": "^4.0.3"
  }
}
